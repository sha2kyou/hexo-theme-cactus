<footer id="footer-extend">
  <% if (theme.icp.enabled || theme.beian.enabled) { %>
    <div class="footer-left">
      <% if (theme.icp.enabled) { %>
        <a href='http://beian.miit.gov.cn/'><%= theme.icp.icp_no %></a>
        <% } %>
      <% if (theme.beian.enabled) { %>
        <a href="http://www.beian.gov.cn/portal/registerSystemInfo?recordcode=<%- theme.beian.beian_id %>"><%= theme.beian.beian_no %></a>
      <% } %>
    </div>
  <% } %>
</footer>
<footer id="footer">
  <div class="footer-left">
    &copy;
    <% var endYear = (theme.copyright && theme.copyright.end_year) ? theme.copyright.end_year : new Date().getFullYear() %>
    <% var startYear = (theme.copyright && theme.copyright.start_year) ? theme.copyright.start_year : new Date().getFullYear() %>
    <%= startYear >= endYear ? endYear : startYear + "-" + endYear %>
    <%= config.author || config.title %>
    <% if (theme.cc.enabled) { %>
      <a href='<%- theme.cc.url %>'><%= theme.cc.name %></a>
    <% } %>
    <!-- Powered by
    <a href="https://hexo.io/" target="_blank" rel="nofollow">hexo</a> -->
  </div>
  <div class="footer-right">
    <nav>
      <ul>
        <% for (var i in theme.nav) { %> 
          <li><a href="<%- url_for(theme.nav[i]) %>"><%= __('nav.'+i).replace("nav.", "") %></a></li>
        <% } %>
      </ul>
    </nav>
  </div>
</footer>
